{
  "name": "timetable-api",
  "version": "1.0.0",
  "description": "Timetable API backend",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db": "node db-manager.js",
    "db:stats": "node db-manager.js stats",
    "db:backup": "node db-manager.js backup --file timetable-backup.db",
    "db:restore": "node db-manager.js restore --file timetable-backup.db",
    "db:teachers": "node db-manager.js teachers list",
    "db:subjects": "node db-manager.js subjects list",
    "db:rooms": "node db-manager.js rooms list",
    "db:classes": "node db-manager.js classes list"
  },
  "keywords": [
    "timetable",
    "api",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.16.5",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
